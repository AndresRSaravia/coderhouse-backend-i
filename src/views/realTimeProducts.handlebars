<div>
	<br>Agregar un producto</br>
    <input type="text" id="messageInput">
    <button id="sendMessage">Enviar</button>
    <h1>Todos los mensajes</h1>
    <div id="messageContainer"></div>
</div>

<h1>Registro de producto</h1>
<label for="title">Nombre:</label>
<input type="text" id="titleInput" name="title" required><br>
<label for="description">Descripción:</label>
<input type="textarea" id="descriptionInput" name="description" required><br>
<label for="code">Código:</label>
<input type="text" id="codeInput" name="code" required><br>
<label for="price">Precio:</label>
<input type="number" id="priceInput" name="price" step="1" min="0" max="100" required><br>
<label for="status">status:</label>
<input type="number" id="statusInput" name="status" step="1" min="0" max="1" required><br>
<label for="stock">Stock:</label>
<input type="number" id="stockInput" name="stock" required><br>
<label for="category">Categoría:</label>
<input type="text" id="categoryInput" name="category" required><br>
<label for="thumbnails">Imagen:</label>
<input type="file" id="thumbnailsInput" name="thumbnails" accept="image/png, image/jpeg" /><br>
<button type="submit" id="sendProduct">Registrar</button>



<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTimeProducts.js"></script>